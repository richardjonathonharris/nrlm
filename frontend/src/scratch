{props.data
                        .sort(function(a, b) {
                            const gamesA = a.gamesPlayed;
                            const gamesB = b.gamesPlayed;

                            let comparison = 0;
                            if (gamesA > gamesB) {
                                comparison = 1;
                            } else if (gamesA < gamesB) {
                                comparison = -1;
                            }
                            return comparison  * -1
                        }
                        )
                        .map(person =>
                            <li key={person.id}>
                                {person.name} -- {person.gamesPlayed} -- {person.totalPoints}
                            </li>
                    )}

